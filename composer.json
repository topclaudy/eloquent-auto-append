{
  "name": "awobaz/eloquent-auto-append",
  "description": "Automatically append accessors to model serialization",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel appends",
    "laravel accessors"
  ],
  "authors": [
    {
      "name": "Claudin J. Daniel",
      "email": "cdaniel@awobaz.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/database": "~5.4|~6.0",
    "illuminate/console": "~5.4|~6.0",
    "illuminate/support": "~5.4|~6.0",
    "illuminate/cache": "~5.4|~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "laravel/laravel": "~5.4|~6.0",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Awobaz\\AutoAppend\\": "src"
    },
    "files": [
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awobaz\\AutoAppend\\AutoAppendServiceProvider"
      ]
    }
  },
  "suggest": {
    "awobaz/eloquent-mutators": "Reusable accessors/mutators (getters/setters) for Laravel 5's Eloquent",
    "awobaz/compoships": "Multi-columns relationships for Laravel 5's Eloquent",
    "awobaz/blade-active": "Blade directives for the Laravel 'Active' package",
    "awobaz/syntactic": "Syntactic sugar for named and indexed parameters call."
  }
}
